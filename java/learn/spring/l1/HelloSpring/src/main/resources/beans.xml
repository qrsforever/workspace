<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd" >
<beans>
	<bean id="springHelloWorld"
		class="com.leeco.learn.spring.helloworld.impl.SpringHelloWorld">
	</bean>
	<bean id="strutsHelloWorld"
		class="com.leeco.learn.spring.helloworld.impl.StrutsHelloWorld">
	</bean>
	<bean id="helloWorldService"
		class="com.leeco.learn.spring.helloworld.HelloWorldService">
		<property name = "helloWorld" ref="strutsHelloWorld" />
	</bean>
</beans>
